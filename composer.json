{
    "name": "matthewtan/userbtob",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.1.0 || ^5.2.0",
        "cakephp/migrations": "^4.0",
        "muffin/trash": "dev-master"
    },
    "require-dev": {
        "cakephp/bake": "3.x-dev",
        "cakephp/cakephp-codesniffer": "5.x-dev",
        "phpstan/phpstan": "2.1.x-dev",
        "phpunit/phpunit": "10.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Matthewtan\\Userbtob\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matthewtan\\Userbtob\\Test\\": "tests/",
            "Matthewtan\\Userbtob\\Test\\Fixture\\": "tests/Fixture/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
