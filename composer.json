{
    "name": "matthewtan/ts-user-b-to-b",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.1.0",
        "cakephp/migrations": "^4.0",
        "muffin/trash": "dev-master"
    },
    "require-dev": {
        "cakephp/bake": "^3.0",
        "cakephp/cakephp-codesniffer": "^5.0",
        "phpstan/phpstan": "^2.1.0",
        "phpunit/phpunit": "^10.5.0"
    },
    "autoload": {
        "psr-4": {
            "TsUserBToB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TsUserBToB\\Test\\": "tests/",
            "TsUserBToB\\Test\\Fixture\\": "tests/Fixture/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
